   <body ng-app='myApp'>
      {{#if_eq menu 'exchange_market'}}
      
      
    <div class="modal fade modal_exchange" id="modal_buy_Exchange" tabindex="-1" role="dialog" aria-labelledby="ModalDeposit-label">
         <div class="modal-dialog" role="document">
            <div class="modal-content">
               <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button> 
                  <h4 class="modal-title" id="ModalDeposit-label">Buy {{MarketName}}</h4>
               </div>
               
            <form id="Form-submit-buy-exchange" method="POST" action="/exchange/submit-buy">
               <input type="hidden" class="token_crt" name="token_crt">
               <input type="hidden" name="MarketName" value="{{MarketName}}" />
               <div class="modal-body">
                  <div class="text-center">
                     <div class="row" style="margin-bottom:20px">
                        <div class="col-sm-3 col-xs-6">
                           <h4 style="font-weight:bold">Type:</h4>
                        </div>
                        <div class="col-sm-3 col-xs-6">
                           <h4 data-bind="text:orderTypeMsg">Buy {{exchange}}</h4>
                        </div>
                        <div class="col-sm-3 col-xs-6">
                           <h4 style="font-weight:bold">Market:</h4>
                        </div>
                        <div class="col-sm-3 col-xs-6">
                           <h4>{{MarketName}}</h4>
                        </div>
                     </div>
                    
                     
                     <div id="form-container">
                        <form role="form" class="form-horizontal" id="submit_buy_exchange">
                           <div class="form-group" style="margin-bottom:10px;float: left;">
                              <h5 class="col-sm-offset-1 col-sm-3">
                                 Quantity:
                              </h5>
                              <div class="col-sm-7">
                                 <div class="input-group">
                                    <input class="form-control text-right" name="quantity" type="text" readonly="readonly" value="0">
                                    <span class="input-group-addon">{{exchange}}</span>
                                 </div>
                              </div>
                           </div>
                           <div class="clearfix"></div>
                           <div class="form-group" style="margin-bottom:10px;float: left;">
                              <h5 class="col-sm-offset-1 col-sm-3">
                                 Price:
                              </h5>
                              <div class="col-sm-7">
                                 <div class="input-group">
                                    <input class="form-control text-right" name="price" type="text"  readonly="readonly" value="0">
                                    <span class="input-group-addon">{{namecoin}}</span>
                                 </div>
                              </div>
                           </div>
                           <div class="clearfix"></div>
                           <div class="form-group" style="margin-bottom:10px;float: left;">
                              <h5 class="col-sm-offset-1 col-sm-3">
                                 Subtotal:
                              </h5>
                              <div class="col-sm-7">
                                 <div class="input-group">
                                    <input class="form-control text-right" name="subtotal" type="text" readonly="readonly" value="0">
                                    <span class="input-group-addon">{{namecoin}}</span>
                                 </div>
                              </div>
                           </div>
                           <div class="clearfix"></div>
                           <div class="form-group" style="margin-bottom:10px;float: left;">
                              <h5 class="col-sm-offset-1 col-sm-3">
                                 Commission:
                              </h5>
                              <div class="col-sm-7">
                                 <div class="input-group">
                                    <input class="form-control text-right" name="commission" type="text" readonly="readonly" value="0">
                                    <span class="input-group-addon">{{namecoin}}</span>
                                 </div>
                              </div>
                           </div>
                           <div class="clearfix"></div>
                           <div class="form-group" style="margin-bottom:10px;float: left;">
                              <h5 class="col-sm-offset-1 col-sm-3">
                                 Total:
                              </h5>
                              <div class="col-sm-7">
                                 <div class="input-group">
                                    <input class="form-control text-right" name="total" type="text" readonly="readonly" value="0">
                                    <span class="input-group-addon">{{namecoin}}</span>
                                 </div>
                              </div>
                           </div>
                        
                     </div>
                     <div class="clearfix"></div>
                     <div class="alert alert-danger alert-dismissable error_submit_buy" style="display: none;"></div>
                     <div class="clearfix"></div>
                     <div class="bs-callout bs-callout-warning">
                        <h4>Disclaimer</h4>
                        <p>Please verify this order before confirming. All orders are final once submitted and we will be unable to issue you a refund.</p>
                     </div>
                  </div>
               </div>
               <div class="modal-footer">
                 <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                 <button type="submit" class="btn btn-primary has-spinner btn-submit-exchain"  style="">
                     <span class="spinner"><i style="display: none;" class="fa fa-spinner fa-pulse"></i></span>
                     <confirm>Confirm</confirm>
                 </button>
             </div>
             <div class="clearfix"></div>
         </form>
            </div>
         </div>
      </div>
      <div class="modal fade modal_exchange" id="modal_sell_Exchange" >
         <div class="modal-dialog" role="document">
            <div class="modal-content">
               <div class="modal-header">
