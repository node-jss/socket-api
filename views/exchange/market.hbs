         <div class="col-md-6 hidden_js" ng-controller="CtrOrderBuy">
            <div class="card">
               <div class="content">
                  <h4 class="title pull-left">Buy Order</h4>
                  <div class="pull-right" style="margin-top: 5px;"><span style="line-height: 30px; margin-right: 5px" class="pull-left">Display</span>
                    <select style="width: 70px; height: 30px;" class="form-control pull-left" ng-model="viewby" ng-change="setItemsPerPage(viewby)">
                      <option>10</option>
                      <option>25</option>
                      <option>50</option>
                      <option>100</option>
                    </select>  

                    <span style="line-height: 30px; margin-left: 5px" class="pull-left">rows</span>
                  </div>



                 <div class="table-exchanges" >
                   <table border="1" cellpadding="0" cellspacing="0"  style="border-collapse:collapse;"  class="table table-striped table-bordered table-hover"  >
                       <thead>
                          <tr>
                            {{#if has_login}}
                              <th class="text-center"><i class="fa fa-caret-down"></i></th>
                            {{/if}}
                             <th class="text-left">Total</th>
                             <th class="text-right">Size ({{exchange}}) </th>
                             <th class="text-right">Bid ({{namecoin}})</th>
                             <!-- <th><i class="fa fa-star"></i></th> -->
                          </tr>
                       </thead>
                       
                       <tbody> 
                      
                          <tr ng-repeat="row in data | orderBy: '-price/10000000' | slice:((currentPage-1)*itemsPerPage):((currentPage)*itemsPerPage)" >
                            {{#if has_login}}
                              <td><i class="fa fa-caret-down" ng-click="ClickBuyOrder(row.total+'_'+row.quantity+'_'+row.price)"></i></td>
                            {{/if}}
                            <td class="text-left" ng-click="ClickBuyForm(row.quantity+'_'+row.price)">{[{(row.total/100000000).toFixed(8)}]}</td>
                            <td class="text-right" ng-click="ClickBuyQuantityForm(row.quantity)">{[{(row.quantity/100000000).toFixed(8)}]}</td>
                            <td class="text-right" ng-click="ClickBuyPriceForm(row.price)">{[{(row.price/100000000).toFixed(8)}]}</td>
                            <!-- <td ng-if="row.user == user_id"><i class="fa fa-star"></i></td>
                            <td ng-if="row.user != user_id"></td> -->
                          </tr>
                          <tr >
                            <td style="text-align: center;" class="text-center" colspan="4" ng-show="data.length == 0">No Data</td>
                          </tr>
                       </tbody>
                    </table>
                    <div class="pull-left">{[{data | sumOfValue:'quantity'}]} {{exchange}}</div>
                    <div class="pull-right">{[{data | sumOfValue:'total'}]} {{namecoin}}</div>
                    <div class="clearfix"></div>
                    <pagination class="pull-right" total-items="totalItems" ng-model="currentPage" max-size="maxSize"  class="pagination-sm" boundary-links="true" items-per-page="itemsPerPage"></pagination>
