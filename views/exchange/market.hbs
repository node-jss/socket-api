                          <input type="hidden" name="MarketName" value="{{MarketName}}" />
                           <div class="form-group">
                              <label for="quantity_Buy" class="col-md-2 control-label">Units</label> 
                              <div class="col-md-10 controls">
                                 <div class="input-group"> <span class="input-group-btn"> <button class="btn btn-primary" style="width:100px;" type="button" ng-click="Max_buy()">Max</button> </span> 
                                 <input type="text" class="form-control text-right" value="0.00000000" name="quantity_Buy"> <span class="input-group-addon">{{exchange}}</span> </div>
                              </div>
                           </div>
                           <div class="form-group">
                              <label for="price_Buy" class="col-md-2 control-label">Bid</label> 



                              <div class="col-md-10 controls">
                                 <div class="input-group">
                                    <div class="input-group-btn" style="width: 100px">
                                     <div class="dropdown">
                                        <button style="width: 100%;border-right: none; border-radius: 0;" class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Bid
                                        <span class="caret"></span></button>
                                        <ul class="dropdown-menu">
                                          <li><a ng-click="Price_last_buy()" href="javascript:void(0)">Last</a></li>
                                          <li><a ng-click="Price_bid_buy()" href="javascript:void(0)">Bid</a></li>
                                          <li><a ng-click="Price_ask_buy()" href="javascript:void(0)">Ask</a></li>
                                        </ul>
                                      </div>
                                    </div>
                                    <input type="text" class="form-control text-right" name="price_Buy" value="0.00000000"> <span class="input-group-addon">{{namecoin}}</span> 
                                 </div>
                              </div>
                           </div>
                           <div class="form-group">
