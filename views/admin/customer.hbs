<div class="content">
   <div class="container-fluid">
      <div class="row">
         <div class="col-md-12">
            <div class="card">
               <div class="content">
                  <div class="material-datatables">
                     <table id="datatables" class="table table-striped table-no-bordered table-hover" style="width:100%;cellspacing:0">
                        <thead>
                           <tr>
                              <th>Date</th>
                              <th>Username</th>
                              <th>Email</th>
                              <!-- <th>Active Email</th> -->
                              <th>Amount BBL</th>
                              <th>Lending</th>
                              <th>Total Profit</th>
                              <th class="disabled-sorting text-right">Actions</th>
                           </tr>
                        </thead>
                        <tbody>
                           {{#each users}} 
                           <tr>
                              <td>{{formatDate this.signupDate "MM/DD/YYYY LT"}} </td>
                              <td>{{this.displayName}}</td>
                              <td>{{this.email}}</td>
                              <!-- <td>
                              	{{#if_eq this.active_email '1'}} 
                                 <span class="label label-success"> Active</span>
                                 {{/if_eq}}

                                 {{#if_eq this.active_email '0'}} 
                                    <span class="label label-default">Not Active</span>
                                 {{/if_eq}}  
                              </td> -->
                              <td>{{satoshi this.balance.coin_wallet.available}} BBL</td>
                              <td>{{this.total_invest}} USD</td>
                              <td>{{this.total_earn}} USD</td>
                              <td class="text-right">
                                 <!-- <a href="#" class="btn btn-simple btn-info btn-icon like"><i class="ti-heart"></i></a> --> <a href="edit/customer/{{this._id}}" class="btn btn-simple btn-warning btn-icon edit"><i class="ti-pencil-alt"></i></a> <!-- <a href="#" class="btn btn-simple btn-danger btn-icon remove"><i class="ti-close"></i></a> --> 
                              </td>
                           </tr>
                           {{/each}} 
                        </tbody>
                     </table>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>